{
    "owner": "tagspaces",
    "repo": "tagspaces",
    "appimage": {
        "rename": "tagspaces",
        "name_template": "{repo}-{latest_version}-{characteristic_suffix}.AppImage",
        "characteristic_suffix": [
            "linux-x86_64",
            "x86_64",
            "amd64"
        ]
    },
    "github": {
        "repo": true,
        "prerelease": false
    },
    "verification": {
        "digest": false,
        "skip": false,
        "checksum_file": "SHA256SUMS.txt",
        "checksum_hash_type": "sha256"
    },
    "icon": {
        "url": "https://raw.githubusercontent.com/tagspaces/tagspaces/master/assets/icon.png",
        "name": "tagspaces.png"
    }
}