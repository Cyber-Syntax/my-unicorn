{
    "owner": "logseq",
    "repo": "logseq",
    "appimage": {
        "rename": "logseq",
        "name_template": "{rename}-linux-x64-{latest_version}.AppImage",
        "characteristic_suffix": [
            "linux-x64",
            "x86_64",
            "amd64"
        ]
    },
    "github": {
        "repo": true,
        "prerelease": false
    },
    "verification": {
        "digest": false,
        "skip": false,
        "checksum_file": "SHA256SUMS.txt",
        "checksum_hash_type": "sha256"
    },
    "icon": {
        "extraction": true,
        "url": "https://raw.githubusercontent.com/logseq/logseq/master/resources/icons/logseq.png",
        "name": "logseq.png"
    }
}