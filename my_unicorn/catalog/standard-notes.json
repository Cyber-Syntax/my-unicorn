{
    "owner": "standardnotes",
    "repo": "app",
    "appimage": {
        "rename": "standard-notes",
        "name_template": "standard-notes-{latest_version}-linux-{characteristic_suffix}.AppImage",
        "characteristic_suffix": [
            "x86_64",
            "amd64"
        ]
    },
    "github": {
        "repo": true,
        "prerelease": false
    },
    "verification": {
        "digest": false,
        "skip": false,
        "checksum_file": "SHA256SUMS",
        "checksum_hash_type": "sha256"
    },
    "icon": {
        "extraction": true,
        "url": "https://raw.githubusercontent.com/standardnotes/app/main/packages/clipper/images/icon128.png",
        "name": "standard-notes.png"
    }
}