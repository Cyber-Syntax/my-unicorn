{
  "config_version": "2.0.0",
  "metadata": {
    "name": "app",
    "display_name": "app",
    "description": ""
  },
  "source": {
    "type": "github",
    "owner": "standardnotes",
    "repo": "app",
    "prerelease": false
  },
  "appimage": {
    "naming": {
