{
  "config_version": "2.0.0",
  "metadata": {
    "name": "weekToDo",
    "display_name": "weekToDo",
    "description": "Minimalist weekly planner and to-do list"
  },
  "source": {
    "type": "github",
    "owner": "manuelernestog",
    "repo": "weekToDo",
    "prerelease": false
  },
  "appimage": {
    "naming": {
      "template": "WeekToDo-{latest_version}.AppImage",
      "target_name": "weektodo",
      "architectures": [
        "amd64",
        "x86_64"
      ]
    }
  },
  "verification": {
    "method": "skip"
  },
  "icon": {
    "method": "extraction",
    "filename": "weektodo.png",
    "download_url": "https://raw.githubusercontent.com/manuelernestog/weekToDo/master/public/icon.png"
  }
}