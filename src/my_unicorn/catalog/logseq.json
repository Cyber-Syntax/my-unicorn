{
  "config_version": "2.0.0",
  "metadata": {
    "name": "logseq",
    "display_name": "logseq",
    "description": "Privacy-first knowledge management and collaboration platform"
  },
  "source": {
    "type": "github",
    "owner": "logseq",
    "repo": "logseq",
    "prerelease": false
  },
  "appimage": {
    "naming": {
      "template": "{rename}-linux-x64-{latest_version}.AppImage",
      "target_name": "logseq",
      "architectures": [
        "amd64",
        "x86_64"
      ]
    }
  },
  "verification": {
    "method": "checksum_file",
    "checksum_file": {
      "filename": "SHA256SUMS.txt",
      "algorithm": "sha256"
    }
  },
  "icon": {
    "method": "extraction",
    "filename": "logseq.png",
    "download_url": "https://raw.githubusercontent.com/logseq/logseq/master/resources/icons/logseq.png"
  }
}